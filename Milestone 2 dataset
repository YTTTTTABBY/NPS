CREATE TABLE StudentLanguages (
    StudentID INT PRIMARY KEY AUTO_INCREMENT,
    Name VARCHAR(50),
    Email VARCHAR(100),
    Major VARCHAR(50),
    Interest VARCHAR(50),
    Language1 VARCHAR(50),
    Language2 VARCHAR(50)
);

