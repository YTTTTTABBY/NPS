CREATE TABLE Tourists (
    Name VARCHAR(50),
    Email VARCHAR(100),
    Nationality VARCHAR(50),
    NumberOfVisitors INT,
    Interests VARCHAR(255),
    LanguagesSpoken VARCHAR(100)
);

INSERT INTO Tourists (Name, Email, Nationality, NumberOfVisitors, Interests, LanguagesSpoken) VALUES
